generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // atau postgresql / sqlite
  url      = env("DATABASE_URL")
}

model Obat {
  id        Int      @id @default(autoincrement())
  nama      String
  stok      Int
  expiredAt DateTime
}

model Transaksi {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
}
